{
    "name": "czim/laravel-modelcomparer",
    "description": "Performs a deep-nested comparison of a model's before/after state.",
    "keywords": [
        "eloquent",
        "model",
        "log"
    ],
    "homepage": "https://github.com/czim/laravel-modelcomparer",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen.zimmerman@endeavour.nl",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "czim/laravel-paperclip": "*",
        "czim/phpunit-printer": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/larastan": "^2.2",
        "orchestra/testbench": "^7.0",
        "orchestra/database": "^7.0",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Czim\\ModelComparer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\ModelComparer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Czim\\ModelComparer\\ModelComparerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
